# tutorial/tut04.hancho

tut04_rules = hancho.load("tut04_rules.hancho")

main_o = hancho(
  tut04_rules.compile_cpp,
  in_src = "src/main.cpp",
)

util_o = hancho(
  tut04_rules.compile_cpp,
  in_src = "src/util.cpp"
)

app = hancho(
  tut04_rules.link_cpp,
  in_objs = [main_o, util_o],
  out_bin = "app"
)
